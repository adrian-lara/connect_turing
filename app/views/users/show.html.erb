<%= render partial: 'shared/show_content' %>

<% if @user == current_user %>
  <%= link_to "Edit", edit_default_user_path(@user), class: "link edit-link" %>
  <%= link_to "Delete", default_user_path(@user), data: { confirm: "You are about to delete user: #{@user.name}.\nThis CANNOT be undone." }, method: :delete, class: "link delete-link" %>
<% end %>
